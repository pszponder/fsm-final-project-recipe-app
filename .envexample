# Specify if the environment is development or production
NODE_ENV = development

# Port which server will be listening on
PORT = 5000

# Connection String for MongoDB
MONGO_URI = mongodb+srv://YOUR-MONGO-URI-CONNECTION-STRING

# Secret which will be used for JWT Authentication
# Node.js command to generate tokens (random strings): require('crypto').randomBytes(64).toString('hex')
JWT_SECRET = abc123
ACCESS_TOKEN_SECRET = abc123
REFRESH_TOKEN_SECRET = abc123